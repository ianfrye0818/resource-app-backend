{
  "name": "resource-app-backend",
  "version": "0.0.1",
  "description": "",
  "author": "Ian Frye",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:generate": "npx prisma generate && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "docker:deploy-prod": "docker-compose up -d",
    "docker:deploy-staging": "docker-compose -f docker-compose.staging.yml up -d",
    "docker:deploy-db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:build-staging": "docker buildx build -t biggiefrye45/resource-apps-backend:staging .",
    "docker:build-prod": "docker buildx build --push -t biggiefrye45/resource-apps-backend:latest .",
    "docker:push-prod": "docker push biggiefrye45/resource-apps-backend:latest",
    "docker:push-staging": "docker push biggiefrye45/resource-apps-backend:staging",
    "db:reset": "prisma migrate reset --force && prisma migrate dev --name init",
    "db:generate": "prisma migrate dev --name init && prisma db push && prisma generate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.25.0",
    "@google/generative-ai": "^0.16.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/throttler": "^6.0.0",
    "@prisma/client": "^5.17.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "docxtemplater": "^3.49.1",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.8.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.54.0",
    "passport": "^0.7.0",
    "passport-cookie": "^1.0.9",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-parse": "^1.1.1",
    "pizzip": "^3.1.7",
    "reflect-metadata": "^0.2.0",
    "resend": "^3.5.0",
    "rxjs": "^7.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pdf-parse": "^1.1.4",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.92.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
